{
  "name": "react-vk",
  "private": false,
  "version": "2.1.2",
  "main": "build/index.js",
  "browserslist": "last 2 versions, ie 10",
  "description": "VK API bindings for React",
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "babel src -d build",
    "test": "jest ./tests/",
    "test:coverage": "jest --coverage ./tests/",
    "test:watch": "jest --watch ./tests/",
    "prepublish": "npm run build",
    "prettier": "prettier --write src/*.js",
    "precommit": "lint-staged"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupJest.js"
    ]
  },
  "lint-staged": {
    "src/*.js": [
      "npm run prettier",
      "git add"
    ],
    "*.md": [
      "npm run prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmarkelov/react-vk"
  },
  "peerDependencies": {
    "react": "16.3.x || || 16.4."
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.55",
    "@babel/core": "7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.55",
    "@babel/preset-env": "7.0.0-beta.55",
    "@babel/preset-react": "7.0.0-beta.55",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "keywords": [
    "react",
    "react-component",
    "vk",
    "react-vk",
    "vk-api",
    "react-vk-api"
  ],
  "author": "Maksim Markelov <mapkeji26@gmail.com>",
  "license": "MIT"
}
